{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "rlox",
      "camelcase": "Rlox",
      "title": "Rlox",
      "scope": "source.lox",
      "highlights": "queries/highlights.scm",
      "file-types": [
        "lox"
      ],
      "injection-regex": "^rlox$",
      "class-name": "TreeSitterRlox"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "A tree-sitter parser for the language rlox",
    "authors": [
      {
        "name": "Abreham Kassa",
        "email": "abrehamkassa19@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/abrishk26/rlox"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}